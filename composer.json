{
    "name": "flopezlosada/calendar-bundle",
    "type": "symfony-bundle",
    "description": "This bundle allows you to integrate the jQuery FullCalendar plugin into your Symfony2 application.",
    "keywords": ["calendar", "fullcalendar", "jquery calendar"],
    "homepage": "https://github.com/flopezlosada/calendar-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Yudin",
            "email": "mikeyudin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/twig-bundle": "~2.8|~3.2|~4.0",
        "symfony/framework-bundle": "~2.7|~3.2|~4.0",
        "friendsofsymfony/jsrouting-bundle": "~2.4",
        "doctrine/collections": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0",
        "symfony/phpunit-bridge": "^3.4|^4.0",
        "symfony/debug": "~3.4|~4.0",
        "symfony/browser-kit" : "^3.4|^4.0",
        "symfony/finder": "^3.4|^4.0"
    },
    "autoload": {
        "psr-0": {
            "ADesigns\\CalendarBundle": ""
        }
    },
    "target-dir": "ADesigns/CalendarBundle"
}
